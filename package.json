{
  "name": "apy-rest2front",
  "private": true,
  "version": "0.8.0",
  "description": "Provide an easy-to-configure system to get a fully functional CRUD views based on a REST API schema",
  "repository": "https://gitlab.com/apysoft/apy-rest2front",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "del": "^2.2.1",
    "eslint-plugin-babel": "^3.3.0",
    "fs": "0.0.2",
    "gulp": "^3.9.1",
    "gulp-bower": "0.0.13",
    "gulp-clean": "^0.3.2",
    "gulp-cli": "^1.2.2",
    "gulp-concat": "^2.6.0",
    "gulp-copy": "0.0.2",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-html-minifier": "^0.1.8",
    "gulp-if": "^2.0.1",
    "gulp-jsdoc3": "^0.3.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-watch": "^4.3.9",
    "html-minifier": "^3.0.2",
    "http-server": "^0.6.1",
    "istanbul": "^0.4.3",
    "jasmine-core": "^2.3.4",
    "jsdoc-rst-template": "0.0.17",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.1.12",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-threshold-reporter": "^0.1.15",
    "minimatch": "^3.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^2.5.1",
    "shelljs": "^0.2.6",
    "webdriver-manager": "^10.1.0"
  },
  "config": {
    "unsafe-perm": true
  },
  "scripts": {
    "postinstall": "./node_modules/bower/bin/bower --allow-root install",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 9000",
    "pretest": "npm install",
    "test": "./node_modules/gulp/bin/gulp.js test",
    "build": "./node_modules/gulp/bin/gulp.js build",
    "test-with-watcher": "./node_modules/karma/bin/karma start karma.conf.js",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "./node_modules/protractor/bin/webdriver-manager update --standalone",
    "preprotractor": "npm run update-webdriver",
    "protractor": "./node_modules/protractor/bin/protractor e2e-tests/protractor.conf.js --standalone"
  },
  "dependencies": {
    "bower": "^1.3.1",
    "base64-regex": "^1.1.1"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "plugins": [
      "babel"
    ],
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always"
      ],
      "no-cond-assign": [
        "error",
        "always"
      ],
      "no-console": "off"
    }
  }
}
